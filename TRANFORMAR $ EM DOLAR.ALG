Algoritmo "Tranformar $ em dolar"


Var
   br,eua,trans: real


Inicio
      escreval("Qual valor a ser convertido ? ")
      leia(Br)
      escreval ("Qual valor do dólar atualmente? ")
      leia(eua)
      
      trans<- (br*eua)
      
      escreval("O valor convertido de reais para dólar é de  ",trans)'

Fimalgoritmo